{
    "ID": "do_TextBox",
    "IsContainer": "0",
    "Type": "UI",
    "Name": "多行文本编辑框",
    "Version": "Internal",
    "Description": "通常只能输入多行数据",
    "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/R/20150617/e96012c5-0f63-40fc-8f06-39d2e4e82be7.png",
    "IOS": {
        "Support": 1,
        "MinVersion": "iOS 7.0"
    },
    "Android": {
        "Support": 1,
        "MinVersion": "4.0"
    },
    "Windows": {
        "Support": 1,
        "MinVersion": "8.1"
    },
    "WindowsPhone": {
        "Support": 1,
        "MinVersion": "8.1"
    },
    "Html": {
        "Support": 1,
        "MinVersion": "5.0"
    },
    "Properties": [
                   {
                   "ID": "enterText",
                   "Name": "按钮文字",
                   "Type": "String",
                   "Type_JavaScript": "String",
                   "DefaultValue": "default",
                   "EditType": "0",
                   "Group": "",
                   "Description": "修改软键盘上按钮显示文字，有以下枚举值：go前往、send发送、next下一项、done完成、search搜索、default由inputType属性控制，与系统相同,仅支持iOS平台",
                   "Design": {
                   "Type": "List",
                   "Data": [
                            {
                            "id": "go",
                            "value": "前往"
                            },
                            {
                            "id": "send",
                            "value": "发送"
                            },
                            {
                            "id": "next",
                            "value": "下一项"
                            },
                            {
                            "id": "done",
                            "value": "完成"
                            },
                            {
                            "id": "search",
                            "value": "搜索"
                            },
                            {
                            "id": "default",
                            "value": "默认"
                            }
                            ]
                   }
                   },
                   {
                   "ID": "inputType",
                   "Name": "输入类型",
                   "Type": "String",
                   "DefaultValue": "ENG",
                   "EditType": "1",
                   "Group": "",
                   "Description": "输入类型，不设置这个属性的时候会使用系统默认键盘支持所有字符，可设置的值包括以下5种:\n\t\r\n\"ASC\" ：支持ASCII的默认键盘\n\r\n\"PHONENUMBER\" ：标准电话键盘，支持＋＊＃字符\n\r\n\"URL\" ：URL键盘，支持.com按钮 只支持URL字符\n\t\t\r\n\"ENG\" :英文键盘\r\n\"DECIMAL\" :数字与小数点键盘（仅支持iOS平台）",
                   "Design": {
                   "Type": "List",
                   "Data": [
                            { "ID": "ASC", "Value": "默认键盘" }
                            ,
                            { "ID": "PHONENUMBER", "Value": "标准电话键盘" }
                            ,
                            { "ID": "URL", "Value": "URL键盘" }
                            ,
                            { "ID": "ENG", "Value": "英文键盘" }
                            ,
                            { "ID": "DECIMAL", "Value": "数字与小数点键盘" }
                            ]
                   }
                   },
                   {
                   "ID": "enabled",
                   "Name": "是否可编辑",
                   "Type": "Bool",
                   "DefaultValue": "true",
                   "EditType": "1",
                   "Group": "",
                   "Description": "控制文本框是否为可编辑状态，为false时不可编辑",
                   "Design": {
                   "Type": "Boolean",
                   "Data": []
                   }
                   },
                   {
                   "ID": "fontColor",
                   "Name": "字体颜色",
                   "Type": "String",
                   "DefaultValue": "000000FF",
                   "EditType": "1",
                   "Group": "",
                   "Description": "设置字体显示颜色，值格式为：8位16进制字符，前6位是RGB颜色值，后两位是透明度（Alpha），例如：000000FF",
                   "Design": {
                   "Type": "Color",
                   "Data": []
                   }
                   },
                   {
                   "ID": "fontSize",
                   "Name": "字体大小",
                   "Type": "Number",
                   "DefaultValue": "17",
                   "EditType": "1",
                   "Group": "",
                   "Description": "",
                   "Design": {
                   "Type": "UInteger",
                   "Data": []
                   }
                   },
                   {
                   "ID": "fontStyle",
                   "Name": "字体风格",
                   "Type": "String",
                   "DefaultValue": "normal",
                   "EditType": "1",
                   "Group": "",
                   "Description": "包含4种类型：normal：常规；bold：粗体；italic：斜体；bold_italic：粗斜体",
                   "Design": {
                   "Type": "List",
                   "Data": [
                            {
                            "id": "normal",
                            "value": "常规"
                            },
                            {
                            "id": "bold",
                            "value": "粗体"
                            },
                            {
                            "id": "italic",
                            "value": "斜体"
                            },
                            {
                            "id": "bold_italic",
                            "value": "粗斜体"
                            }
                            ]
                   }
                   },
                   {
                   "ID": "hint",
                   "Name": "text为空提示文本",
                   "Type": "String",
                   "DefaultValue": "",
                   "EditType": "1",
                   "Group": "",
                   "Description": "",
                   "Design": {
                   "Type": "Text",
                   "Data": []
                   }
                   },
                   {
                   "ID": "maxLength",
                   "Name": "可允许输入的最大长度",
                   "Type": "Number",
                   "DefaultValue": "100",
                   "EditType": "0",
                   "Group": "",
                   "Description": "",
                   "Design": {
                   "Type": "UInteger",
                   "Data": []
                   }
                   },
                   {
                   "ID": "text",
                   "Name": "文本内容",
                   "Type": "String",
                   "DefaultValue": "",
                   "EditType": "1",
                   "Group": "",
                   "Description": "获取或设置与此控件关联的文本",
                   "Design": {
                   "Type": "Text",
                   "Data": []
                   }
                   },
                   {
                   "ID": "textFlag",
                   "Name": "字体标示",
                   "Type": "String",
                   "DefaultValue": "normal",
                   "EditType": "0",
                   "Group": "",
                   "Description": "包含3种类型：normal：常规；underline：下划线；strikethrough ：删除线",
                   "Design": {
                   "Type": "List",
                   "Data": [
                            {
                            "id": "normal",
                            "value": "常规"
                            },
                            {
                            "id": "underline",
                            "value": "下划线"
                            },
                            {
                            "id": "strikethrough",
                            "value": "删除线"
                            }
                            ]
                   }
                   }
                   ],
    "Events": [
               {
               "ID": "enter",
               "Description": "点击键盘右下角按钮时触发,仅支持iOS平台",
               "ReturnType": "",
               "ReturnDesc": ""
               },
               {
               "ID": "focusIn",
               "Description": "进入编辑状态",
               "ReturnType": "Node",
               "ReturnDesc": "返回键盘高度，{keybordHeight:'25'}"
               },
               {
               "ID": "focusOut",
               "Description": "离开编辑状态",
               "ReturnType": "String",
               "ReturnDesc": ""
               },
               {
               "ID": "textChanged",
               "Description": "文字变化时触发",
               "ReturnType": "String",
               "ReturnDesc": ""
               }
               ],
    "syncMethodes": [
                     {
                     "ID": "setFocus",
                     "Name": "设置焦点",
                     "Description": "设置是否得到焦点，得到焦点时软键盘弹出，失去焦点时软键盘收起",
                     "ReturnType": "",
                     "ReturnDesc": "",
                     "Paras": [
                               {
                               "ID": "value",
                               "Name": "焦点值",
                               "Type": "Bool",
                               "Required": 1,
                               "DefaultValue": "false",
                               "Description": "为true时有焦点，false时无焦点"
                               }
                               ]
                     },
                     {
                     "ID": "setSelection",
                     "Name": "设置光标位置",
                     "Description": "",
                     "ReturnType": "",
                     "ReturnDesc": "",
                     "Paras": [
                               {
                               "ID": "position",
                               "Name": "光标位置",
                               "Type": "Number",
                               "Required": 1,
                               "DefaultValue": "0",
                               "Description": "表示在已有字符的第几个位置，从0开始，比如文本框内有5个字符，position设置为3，光标即在第4个字符之前"
                               }
                               ]
                     }
                     ],
    "asyncMethodes": []
}
